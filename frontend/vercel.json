{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "installCommand": "npm install",
          "buildCommand": "npm run build",
          "outputDirectory": "dist"
        }
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/assets/(.*)",
        "dest": "/assets/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
}